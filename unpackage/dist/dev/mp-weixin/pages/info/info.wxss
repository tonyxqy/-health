
image {
		border: 0;
}
.jq22-user-img image {
		border-radius: 72px;
}
.clearfix::after {
		clear: both;
		content: '.';
		display: block;
		height: 0;
		visibility: hidden;
}
.clearfix {}
.viewHeight {
		width: 100%;
		height: 8px;
		background: #f5f5f5;
		position: relative;
		overflow: hidden;
}
.r-line {
		position: relative;
}
.r-line:after {
		content: '';
		position: absolute;
		z-index: 0;
		top: 0;
		right: 0;
		height: 100%;
		border-right: 1px solid #d9d9d9;
		-webkit-transform: scaleX(0.5);
		transform: scaleX(0.5);
		-webkit-transform-origin: 100% 0;
		transform-origin: 100% 0;
}
.b-line {
		position: relative;
}
.b-line:after {
		content: '';
		position: absolute;
		z-index: 2;
		bottom: 0;
		left: 0;
		width: 100%;
		height: 1px;
		border-bottom: 1px solid #e2e2e2;
		-webkit-transform: scaleY(0.5);
		transform: scaleY(0.5);
		-webkit-transform-origin: 0 100%;
		transform-origin: 0 100%;
}
.jq22-flex {
		display: flex;
		align-items: center;
		padding: 15px;
		position: relative;
}
.jq22-flexbg {
		background-color: #fff8ee;
		border-top: 1px solid #ebebeb;
		border-bottom: 1px solid #ebebeb;
}
.jq22-flex-box {
		flex: 1;
		min-width: 0;
		font-size: 14px;
		color: #333;
}

	/* 必要布局样式css */
.jq22-flexView {
		width: 100%;
		height: 100%;
		margin: 0 auto;
		display: flex;
		flex-direction: column;
}
.jq22-scrollView {
		width: 100%;
		height: 100%;
		flex: 1;
		overflow-y: auto;
		overflow-x: hidden;
		-webkit-overflow-scrolling: touch;
		position: relative;
}
.jq22-navBar {
		height: 44px;
		position: relative;
		display: flex;
		z-index: 1002;
		background: #fff;
}
.jq22-navBar-item {
		height: 44px;
		min-width: 25%;
		flex: 0 0 25%;
		padding: 0 0.9rem;
		display: flex;
		align-items: center;
		font-size: 0.7rem;
		white-space: nowrap;
		overflow: hidden;
		color: #808080;
		position: relative;
}
.jq22-navBar-item:first-child {
		order: 1;
		margin-right: -25%;
		font-size: 0.9rem;
		font-weight: bold;
}
.jq22-navBar-item:last-child {
		order: 3;
		justify-content: flex-end;
}
.jq22-center {
		order: 2;
		display: flex;
		justify-content: center;
		align-items: center;
		height: 44px;
		width: 50%;
		margin-left: 25%;
}
.jq22-center-title {
		text-align: center;
		width: 100%;
		white-space: nowrap;
		overflow: hidden;
		display: block;
		text-overflow: ellipsis;
		font-size: 0.95rem;
		color: #333;
}
.icon {
		width: 20px;
		height: 20px;
		display: block;
		border: none;
		float: left;
		background-size: 20px;
		background-repeat: no-repeat;
}
.icon-return {
		background-image: url('data:image/png;base64,');
}
.m-slider {
		overflow-x: hidden;
		width: 100%;
		position: relative;
}
.slider-wrapper {
		display: flex;
		width: 100%;
		height: 100%;
		-webkit-transform: translate3d(0px, 0px, 0px);
		transform: translate3d(0px, 0px, 0px);
		position: relative;
		z-index: 1;
		transition-property: -webkit-transform;
		transition-property: transform;
		transition-property: transform, -webkit-transform;
}
.slider-item {
		width: 100%;
		height: 100%;
		flex-shrink: 0;
		background: #f6f6f6;
}
.slider-item>image {
		width: 100%;
		height: auto;
		display: block;
		border: none;
}
.slider-pagination {
		text-align: right;
		position: absolute;
		width: 100%;
		z-index: 2;
		right: 0;
		bottom: 10px;
		pointer-events: none;
		display: flex;
		align-items: flex-end;
		justify-content: center;
}
.slider-pagination>.slider-pagination-item {
		margin: 0 0.25rem;
		width: 8px;
		height: 8px;
		display: inline-block;
		border-radius: 100%;
		background-color: rgba(255, 255, 255, 0.4);
}
.slider-pagination>.slider-pagination-item.slider-pagination-item-active {
		background-color: #fff;
		border-radius: 100%;
}
.jq22-palace {
		padding: 0.5rem 0;
		position: relative;
		overflow: hidden;
}
.jq22-palace-grid {
		position: relative;
		float: left;
		padding: 1px;
		width: 20%;
		box-sizing: border-box;
		margin: 5px 0;
}
.jq22-palace-grid-icon {
		width: 30px;
		height: 30px;
		margin: 0 auto;
}
.jq22-palace-grid-icon image {
		display: block;
		width: 100%;
		height: 100%;
		border: none;
}
.jq22-palace-grid-text {
		display: block;
		text-align: center;
		color: #333;
		font-size: 0.85rem;
		white-space: nowrap;
		text-overflow: ellipsis;
		overflow: hidden;
		padding-top: 0.2rem;
}
.jq22-palace-grid-text text {
		font-size: 0.8rem;
		font-weight: normal;
		color: #666666;
}
.jq22-user-img {
		width: 60px;
		height: 60px;
		margin-right: 1rem;
}
.jq22-user-img image {
		width: 100%;
		height: auto;
		display: block;
		border: none;
}
.jq22-flex-box text {
		font-size: 1.1rem;
		color: #000000;
		font-weight: normal;
}
.jq22-arrow {
		text-align: right;
		color: #394154;
		padding-right: 12px;
		position: relative;
		font-size: 0.85rem;
}
.jq22-arrow ._span {
		color: #333;
		font-size: 0.85rem;
}
.jq22-arrow:after {
		content: ' ';
		display: inline-block;
		height: 8px;
		width: 8px;
		border-width: 2px 2px 0 0;
		border-color: #cdcdcd;
		border-style: solid;
		-webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
		transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
		position: relative;
		top: -2px;
		position: absolute;
		top: 50%;
		margin-top: -5px;
		right: 2px;
		border-radius: 1px;
}
.jq22-mine-cell {
		/* Safari 5.1 - 6.0 */
		/* Opera 11.1 - 12.0 */
		/* Firefox 3.6 - 15 */
		background: linear-gradient(to right, #63cc83, #00967a);
		/* 标准的语法 */
		margin: 0 0.4rem;
		border-radius: 10px;
		text-align: center;
		padding: 1rem 0;
		box-shadow: 0 3px 10px #ccfdd7;
		-webkit-box-shadow: 0 3px 10px #ccfdd7;
}
.jq22-mine-cell text {
		color: rgba(255, 255, 255, 0.98);
		margin-bottom: 0.3rem;
		font-weight: bold;
}
.jq22-flex-box .jq22-mine-cell text {
		color: rgba(255, 255, 255, 0.98);
}
.jq22-mine-cell-one {
		/* Safari 5.1 - 6.0 */
		/* Opera 11.1 - 12.0 */
		/* Firefox 3.6 - 15 */
		background: linear-gradient(to right, #7eb1fe, #b9e5ff);
		/* 标准的语法 */
		box-shadow: 0 3px 10px #bfe8ff;
		-webkit-box-shadow: 0 3px 10px #bfe8ff;
}
.jq22-flex-one {
		padding: 15px 10px;
}
.jq22-cou-img {
		width: 20px;
		height: 20px;
		margin-right: 0.5rem;
}
.jq22-cou-img image {
		width: 20px;
		height: 20px;
		display: block;
		border: none;
}
.jq22-flex-box text {
		font-size: 0.9rem;
		color: #000000;
		font-weight: 500;
}
.jq22-mine-list .b-line:after {
		left: 45px;
}
.jq22-mine-box {
		margin-bottom: 6rem;
		background-color: white;
}
.jq22-footer {
		width: 100%;
		position: relative;
		z-index: 112;
		display: flex;
		align-items: center;
		padding: 7px 5px 7px 5px;
		background: #ffffff;
}
.jq22-tabBar-item {
		flex: 1;
		display: flex;
		flex-direction: column;
		justify-content: center;
		align-items: center;
		color: #979797;
}
.jq22-tabBar-item-text {
		display: inline-block;
		font-size: 0.65rem;
		color: #acaeba;
		padding-top: 2px;
}
.jq22-tabBar-item-active .jq22-tabBar-item-text {
		color: #3f4659;
}

